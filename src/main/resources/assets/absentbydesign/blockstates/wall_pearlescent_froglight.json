{
  "multipart": [
    {   "when": { "up": "true" },
      "apply": { "model": "absentbydesign:block/wall_pearlescent_froglight_post" }
    },
    {   "when": { "north": "low" },
      "apply": { "model": "absentbydesign:block/wall_pearlescent_froglight_side", "uvlock": true }
    },
    {   "when": { "east": "low" },
      "apply": { "model": "absentbydesign:block/wall_pearlescent_froglight_side", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "low" },
      "apply": { "model": "absentbydesign:block/wall_pearlescent_froglight_side", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "low" },
      "apply": { "model": "absentbydesign:block/wall_pearlescent_froglight_side", "y": 270, "uvlock": true }
    },
    {
      "when": {
        "north": "tall"
      },
      "apply": {
        "model": "absentbydesign:block/wall_pearlescent_froglight_side_tall",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "tall"
      },
      "apply": {
        "model": "absentbydesign:block/wall_pearlescent_froglight_side_tall",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "tall"
      },
      "apply": {
        "model": "absentbydesign:block/wall_pearlescent_froglight_side_tall",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "tall"
      },
      "apply": {
        "model": "absentbydesign:block/wall_pearlescent_froglight_side_tall",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}
